#include <iostream>
using namespace std;
int main() {
    char c;
    cout<<"Enter character"<<endl;
    cin>>c;
    
    if(c=='a'||c=='e'||c=='i'||c=='o'||c=='u'||c=='A'||c=='E'||c=='I'||c=='O'||c=='U'){
        cout<<"Character is a vowel"<<endl;
    }
    else{
    cout<<"Character is a consonant"<<endl;}
    
    return 0;
}

// Output:
//Enter character
//F
//Caracter is a consonant
